{
  "version": 2,
  "installCommand": "cd gosave-frontend && npm install",
  "buildCommand": "cd gosave-frontend && npm run build",
  "outputDirectory": "gosave-frontend/dist",
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
