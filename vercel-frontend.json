{
  "version": 2,
  "name": "gosave-frontend",
  "builds": [
    {
      "src": "gosave-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "gosave-frontend/dist"
      }
    }
  ],
  "installCommand": "cd gosave-frontend && npm install",
  "buildCommand": "cd gosave-frontend && npm run build",
  "outputDirectory": "gosave-frontend/dist",
  "framework": "vite",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "@vite_api_url",
    "VITE_SUPABASE_URL": "@vite_supabase_url",
    "VITE_SUPABASE_ANON_KEY": "@vite_supabase_anon_key",
    "VITE_NODE_ENV": "production"
  }
}
